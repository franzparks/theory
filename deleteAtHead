// java.util.* has been imported for this problem.
// You don't need any other imports.

public ListNode deleteAtHead(ListNode head) {
                
    if(head == null || head.next == head) return null;
    ListNode cur = head;
    ListNode newHead = head.next;
     
    while(cur.next != head){
        cur = cur.next;
    }           

    cur.next = newHead;
    cur = newHead;
     
    return cur;
    
}
